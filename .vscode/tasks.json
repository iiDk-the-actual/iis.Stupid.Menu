{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-release",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "--configuration", "Release", "-v:n"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },

    {
      "label": "build-debug",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "--configuration", "Debug", "-v:n"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": false
      }
    }
  ]
}
